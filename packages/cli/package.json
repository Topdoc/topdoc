{
  "name": "topdoc",
  "description": "Generate usage guides for css",
  "version": "2.0.2",
  "main": "./bin/topdoc",
  "bin": {
    "topdoc": "./bin/index.js"
  },
  "scripts": {
    "test": "ava",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topcoat/topdoc.git"
  },
  "keywords": [
    "css",
    "style",
    "documentation",
    "usage"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/topcoat/topdoc/issues"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/**/*"
    ]
  },
  "nyc": {
    "include": [
      "bin/*.js",
      "topdoc-lib.js"
    ]
  },
  "preferGlobal": "true",
  "dependencies": {
    "commander": "^2.15.1",
    "commander-rc-adapter": "^0.1.1",
    "config-attendant": "^0.1.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "pkginfo": "^0.4.1",
    "postcss": "^6.0.21",
    "postcss-topdoc": "^1.0.3",
    "pug": "^2.0.3",
    "resolve": "^1.1.7",
    "topdoc-default-template": "^0.1.2"
  },
  "devDependencies": {
    "nixt": "^0.5.1",
    "randomstring": "^1.1.5",
    "topdoc-theme": "^0.7.0"
  },
  "gitHead": "bd615c2be5e42174c8f68dc3f1e52b84e5e4c5da"
}

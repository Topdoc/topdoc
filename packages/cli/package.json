{
  "name": "topdoc",
  "description": "Generate usage guides for css",
  "version": "1.1.4",
  "main": "./lib/topdoc",
  "bin": {
    "topdoc": "./bin/index.js"
  },
  "scripts": {
    "prepare": "npm run compile",
    "compile": "babel -d ./ ./src/",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topcoat/topdoc.git"
  },
  "keywords": [
    "css",
    "style",
    "documentation",
    "usage"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/topcoat/topdoc/issues"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "preferGlobal": "true",
  "dependencies": {
    "commander": "^2.15.1",
    "commander-rc-adapter": "^0.1.1",
    "config-attendant": "^0.1.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "pkginfo": "^0.4.1",
    "postcss": "^6.0.21",
    "postcss-topdoc": "^0.1.2",
    "pug": "^2.0.3",
    "resolve": "^1.1.7",
    "topdoc-default-template": "^0.1.2"
  },
  "devDependencies": {
    "eslint-config-garthdb": "^0.1.0",
    "nixt": "^0.5.0",
    "randomstring": "^1.1.5",
    "topdoc-theme": "^0.7.0"
  }

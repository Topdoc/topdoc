{
  "name": "postcss-topdoc",
  "version": "0.1.5",
  "description": "Topdoc parser built in PostCSS",
  "main": "lib/",
  "scripts": {
    "prepare": "npm run compile",
    "compile": "babel -d ./lib/ ./src/",
    "test": "ava",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarthDB/postcss-topdoc.git"
  },
  "keywords": [
    "topdoc",
    "css",
    "documentation",
    "postcss"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GarthDB/postcss-topdoc/issues"
  },
  "homepage": "https://github.com/GarthDB/postcss-topdoc#readme",
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "clean-css": "^4.1.7",
    "debug": "^3.1.0",
    "js-yaml": "^3.9.1",
    "postcss": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "atomdoc-cli": "^1.1.0",
    "ava": "1.0.0-beta.6",
    "nyc": "^12.0.2"
  }
}

{
  "name": "postcss-topdoc",
  "version": "0.1.2",
  "description": "Topdoc parser built in PostCSS",
  "main": "lib/",
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel -d ./lib/ ./src/",
    "test": "nyc ava",
    "lint": "eslint .",
    "report-coverage": "nyc report -r lcovonly && codecov",
    "checkdocs": "atomdoc src",
    "validate": "npm run lint && npm run test && npm run checkdocs",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarthDB/postcss-topdoc.git"
  },
  "keywords": [
    "topdoc",
    "css",
    "documentation",
    "postcss"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GarthDB/postcss-topdoc/issues"
  },
  "homepage": "https://github.com/GarthDB/postcss-topdoc#readme",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "clean-css": "^4.1.7",
    "debug": "^3.0.0",
    "js-yaml": "^3.9.1",
    "postcss": "^6.0.9"
  },
  "devDependencies": {
    "atomdoc-cli": "^1.1.0",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "codecov": "^3.0.0",
    "commitizen": "^2.8.6",
    "cz-customizable": "^4.0.0",
    "eslint": "^4.4.1",
    "eslint-plugin-async-await": "0.0.0",
    "nyc": "^11.1.0",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
